<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="411dp"
        android:layout_height="162dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/dashboard" />

    <TextView
        android:id="@+id/dashboard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/garamond"
        android:text="Dashboard"
        android:textColor="@color/white"
        android:textSize="42dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.287"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.504" />

    <ImageButton
        android:id="@+id/art_gallery_btn"
        android:layout_width="342dp"
        android:layout_height="112dp"
        android:layout_marginTop="28dp"
        android:backgroundTint="#D6B7A7"
        android:scaleType="fitStart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.449"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@drawable/artgallery" />

    <ImageButton
        android:id="@+id/wwi_btn"
        android:layout_width="342dp"
        android:layout_height="112dp"
        android:layout_marginTop="20dp"
        android:backgroundTint="#D6B7A7"
        android:scaleType="fitStart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.449"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/art_gallery_btn"
        app:srcCompat="@drawable/wwi" />

    <ImageButton
        android:id="@+id/space_btn"
        android:layout_width="342dp"
        android:layout_height="112dp"
        android:layout_marginTop="20dp"
        android:backgroundTint="#D6B7A7"
        android:scaleType="fitStart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.449"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wwi_btn"
        app:srcCompat="@drawable/space" />

    <ImageButton
        android:id="@+id/visual_show_btn"
        android:layout_width="342dp"
        android:layout_height="112dp"
        android:layout_marginTop="20dp"
        android:backgroundTint="#D6B7A7"
        android:scaleType="fitStart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.449"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/space_btn"
        app:srcCompat="@drawable/visualshow" />

    <TextView
        android:id="@+id/art_gallery_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/garamond"
        android:text="Art Gallery"
        android:textColor="@color/white"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/art_gallery_btn"
        app:layout_constraintEnd_toEndOf="@+id/art_gallery_btn"
        app:layout_constraintHorizontal_bias="0.737"
        app:layout_constraintStart_toStartOf="@+id/art_gallery_btn"
        app:layout_constraintTop_toTopOf="@+id/art_gallery_btn"
        app:layout_constraintVertical_bias="0.493" />

    <TextView
        android:id="@+id/wwi_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/garamond"
        android:text="WWI"
        android:textColor="@color/white"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/wwi_btn"
        app:layout_constraintEnd_toEndOf="@+id/wwi_btn"
        app:layout_constraintHorizontal_bias="0.668"
        app:layout_constraintStart_toStartOf="@+id/wwi_btn"
        app:layout_constraintTop_toTopOf="@+id/wwi_btn"
        app:layout_constraintVertical_bias="0.518" />

    <TextView
        android:id="@+id/Space_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/garamond"
        android:text="Exploring the Space"
        android:textColor="@color/white"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/space_btn"
        app:layout_constraintEnd_toEndOf="@+id/space_btn"
        app:layout_constraintHorizontal_bias="0.857"
        app:layout_constraintStart_toStartOf="@+id/space_btn"
        app:layout_constraintTop_toTopOf="@+id/space_btn"
        app:layout_constraintVertical_bias="0.481" />

    <TextView
        android:id="@+id/visual_show_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/garamond"
        android:text="Visual Show"
        android:textColor="@color/white"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/visual_show_btn"
        app:layout_constraintEnd_toEndOf="@+id/visual_show_btn"
        app:layout_constraintHorizontal_bias="0.729"
        app:layout_constraintStart_toStartOf="@+id/visual_show_btn"
        app:layout_constraintTop_toTopOf="@+id/visual_show_btn"
        app:layout_constraintVertical_bias="0.481" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="36dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.513"
        app:srcCompat="@drawable/logocircle" />

</androidx.constraintlayout.widget.ConstraintLayout>